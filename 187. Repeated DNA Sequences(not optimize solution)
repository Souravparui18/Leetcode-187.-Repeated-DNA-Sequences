class Solution {
    public List<String> findRepeatedDnaSequences(String s) {
     int n  = s.length();

        HashSet<String>seen = new HashSet<>();
        HashSet<String>result = new HashSet<>();
        for(int i = 0; i <= n-10;i++) {
            String c = s.substring(i,i+10);

            if(seen.contains(c)) {
                result.add(c);
            } else {
                seen.add(c);
            } 
        }
        return new ArrayList<String>(result);
        
    }
}
